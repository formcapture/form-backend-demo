{
  "title": "Brunnen",
  "description": "Brunnen in der Stadt",
  "dataSource": {
    "tableName": "fountains",
    "idColumn": "id",
    "lookupTables": {
      "type": {
        "dataSource": {
          "tableName": "fountain_types",
          "idColumn": "id"
        },
        "includedProperties": ["id"],
        "properties": {
          "id": {
            "resolveAsEnum": true,
            "resolveToColumn": "type"
          }
        }
      }
    },
    "joinTables": {
      "contacts": {
        "relationship": "manyToMany",
        "via": {
          "tableName": "fountains_contacts",
          "schema": "formbackend"
        },
        "on": {
          "self": "fountain_id",
          "other": "contact_id"
        },
        "dataSource": {
          "tableName": "contacts",
          "idColumn": "id"
        },
        "includedProperties": [
          "id",
          "name",
          "phone",
          "picture"
        ],
        "properties": {
          "id": {
            "readOnly": true,
            "options": {
              "hidden": true
            }
          },
          "picture": {
            "title": "Bild",
            "media": {
              "binaryEncoding": "base64"
            },
            "options": {
              "mime_type": [
                "image/png",
                "image/jpg",
                "image/jpeg",
                "application/pdf"
              ],
              "max_upload_size": 5000000
            }
          }
        }
      },
      "fountains_pictures": {
        "relationship": "oneToMany",
        "dataSource": {
          "tableName": "fountains_pictures",
          "idColumn": "id"
        },
        "on": {
          "self": "fountain_id"
        },
        "includedProperties": [
          "id",
          "img_path"
        ],
        "properties": {
          "id": {
            "options": {
              "hidden": true
            }
          },
          "img_path": {
            "title": "Bild",
            "media": {
              "binaryEncoding": "base64"
            },
            "options": {
              "mime_type": [
                "image/png",
                "image/jpg",
                "image/jpeg",
                "application/pdf"
              ]
            }
          }
        }
      },
      "inspections": {
        "relationship": "oneToMany",
        "dataSource": {
          "tableName": "inspections",
          "idColumn": "id",
          "joinTables": {
            "contacts": {
              "relationship": "manyToMany",
              "dataSource": {
                "tableName": "contacts",
                "idColumn": "id"
              },
              "via": {
                "tableName": "inspections_contacts",
                "schema": "formbackend"
              },
              "on": {
                "self": "inspection_id",
                "other": "contact_id"
              },
              "includedProperties": [
                "id"
              ],
              "properties": {
                "id": {
                  "title": "Name",
                  "resolveAsEnum": true,
                  "resolveToColumn": "name"
                }
              }
            }
          }
        },
        "on": {
          "self": "fountain_id"
        },
        "includedProperties": [
          "id",
          "title",
          "description",
          "contacts"
        ],
        "properties": {
          "id": {
            "options": {
              "hidden": true
            }
          },
          "contacts": {
            "title": "Kontakte",
            "resolveTable": true,
            "format": "table",
            "items": {
              "title": "Kontakt"
            }
          }
        }
      }
    }
  },
  "views": {
    "table": true,
    "item": true
  },
  "access": {
    "read": ["fountains_editor"],
    "write": ["fountains_editor"]
  },
  "includedProperties": [
    "id",
    "name",
    "contacts",
    "fountains_pictures",
    "type",
    "inspections"
  ],
  "properties": {
    "id": {
      "readOnly": true,
      "description": ""
    },
    "type": {
      "title": "Brunnentyp",
      "resolveLookup": true
    },
    "contacts": {
      "title": "Kontakte",
      "resolveTable": true,
      "items": {
        "title": "Kontakt"
      }
    },
    "fountains_pictures": {
      "resolveTable": true,
      "title": "Bilder",
      "items": {
        "title": "Bild"
      }
    },
    "inspections": {
      "title": "Inspektionen",
      "resolveTable": true,
      "items": {
        "title": "Inspektion"
      }
    }
  }
}
